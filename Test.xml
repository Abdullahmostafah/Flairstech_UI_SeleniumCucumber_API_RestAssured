<!DOCTYPE suite SYSTEM "https://testng.org/testng-1.0.dtd">

<suite name="Suite" parallel="tests" thread-count="4">

    <listeners>
        <listener class-name="Reports.ExtentReportListener"/>
    </listeners>

    <test name="UITest">
        <classes>
            <class name="EndPoints.Status"/>
        </classes>
    </test>

    <test name="BackendTest">
        <classes>
            <class name="EndPoints.GetTokenAPI"/>
            <class name="EndPoints.AddCandidateAPI"/>
            <class name="EndPoints.DeleteCandidateAPI"/>
        </classes>
    </test>


</suite>
